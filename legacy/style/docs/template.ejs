<!DOCTYPE html>

<html lang="en" ng-app="app">

<head>
        <!-- meta content -->
        <meta content="width=device-width, initial-scale=1" name="viewport" />
        <meta http-equiv="X-UA-Compatible"
              content="IE=11">
        <meta charset="utf-8">
        <meta name="description"
              content="">
        <meta name="keywords"
              content="">       
        <title>SEMOSS</title>             
        <!-- Polyfills must be loaded first-->
        <%= htmlWebpackPlugin
            .tags.bodyTags
                  .filter(function (tag) {
                        return tag.attributes.src.indexOf('polyfills') > -1
                  })
                  .map(function (tag) {
                        return '<script ' + 'src="' + tag.attributes.src + '"></script>'
                  })
                  .join('')
      %>
        <%= htmlWebpackPlugin.tags.headTags %>
        
</head>

<body ng-cloak>
    <home></home>

      <%= htmlWebpackPlugin
            .tags.bodyTags
                  .filter(function (tag) {
                        return tag.attributes.src.indexOf('polyfills') === -1
                  })
                  .map(function (tag) {
                        return '<script ' + 'src="' + tag.attributes.src + '"></script>'
                  })
                  .join('')
      %>
     

</body>

</html>